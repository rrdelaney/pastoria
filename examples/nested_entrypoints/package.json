{
  "name": "pastoria-starter-nested-entrypoints",
  "version": "0.1.5",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier -w .",
    "generate": "grats && relay-compiler && pastoria generate --skip-schema",
    "build": "pnpm generate && pastoria build",
    "dev": "pastoria dev",
    "start": "NODE_ENV=production pastoria-server",
    "check": "tsc"
  },
  "imports": {
    "#src/*": "./src/*",
    "#genfiles/*": "./__generated__/*"
  },
  "dependencies": {
    "dotenv": "catalog:",
    "express": "catalog:",
    "graphql": "catalog:",
    "grats": "catalog:",
    "pastoria-runtime": "workspace:*",
    "pastoria-server": "workspace:*",
    "radix3": "^1.1.2",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-relay": "catalog:",
    "relay-runtime": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/express": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-relay": "catalog:",
    "@types/relay-runtime": "catalog:",
    "babel-plugin-react-compiler": "^1.0.0",
    "pastoria": "workspace:*",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "relay-compiler": "catalog:",
    "relay-config": "^12.0.1",
    "tailwindcss": "catalog:",
    "typescript": "catalog:"
  }
}
